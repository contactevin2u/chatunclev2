{
  "name": "@chatuncle/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:prod": "tsc && npm run db:push",
    "start": "node dist/index.js",
    "lint": "eslint src/",

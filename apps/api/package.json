    "bcryptjs": "^2.4.3",
    "bullmq": "^5.12.0",
    "cloudinary": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.28.0",
    "drizzle-orm": "^0.36.0",
    "express": "^4.18.2",
    "grammy": "^1.21.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
